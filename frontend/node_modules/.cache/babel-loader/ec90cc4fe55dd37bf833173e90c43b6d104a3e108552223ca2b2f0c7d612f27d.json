uter.push('/login');\n      } catch (error) {\n        console.error(\"Erreur lors de la déconnexion :\", error.response ? error.response.data : error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","data","board","initializeBoard","pieceImages","wr","wn","wb","wq","wk","wp","br","bn","bb","bq","bk","bp","mounted","methods","playerColor","reverse","logout","post","localStorage","removeItem","$router","push","error","console","response","message"],"sources":["C:\\wamp64\\www\\Chess\\frontend\\src\\components\\GameBoard.vue"],"sourcesContent":["<template>\r\n    <div class=\"game-container\">\r\n      <aside class=\"sidebar\">\r\n        <nav>\r\n          <ul>\r\n            <li><router-link to=\"/menu\">Accueil</router-link></li>\r\n            <li><router-link to=\"/parametres\">Paramètres</router-link></li>\r\n            <li><a href=\"#\" @click.prevent=\"logout\">Déconnexion</a></li>\r\n          </ul>\r\n        </nav>\r\n      </aside>\r\n      <div class=\"board-container\">\r\n        <h2>Partie d'Échecs</h2>\r\n        <p>Code de la partie : {{ gameCode }}</p>\r\n        <p>Vous jouez les : {{ playerColor === 'w' ? 'Blancs' : 'Noirs' }}</p>\r\n        <div id=\"chessboard\" class=\"chessboard\">\r\n          <div\r\n            v-for=\"(square, index) in board\"\r\n            :key=\"index\"\r\n            :class=\"['square', (index + Math.floor(index / 8)) % 2 === 0 ? 'beige' : 'brown']\"\r\n          >\r\n            <img v-if=\"square\" :src=\"pieceImages[square]\" :alt=\"square\" class=\"piece\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: \"GameBoard\",\r\n  props: ['gameCode', 'playerColor'],\r\n  data() {\r\n    return {\r\n      board: this.initializeBoard(),\r\n      pieceImages: {\r\n        wr: \"src/assets/img/pions/w/rook.png\",\r\n        wn: \"src/assets/img/pions/w/knight.png\",\r\n   