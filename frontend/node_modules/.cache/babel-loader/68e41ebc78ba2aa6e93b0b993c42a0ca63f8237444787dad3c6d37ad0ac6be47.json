{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    ...mapActions(['setUser']),\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8000/login/', {\n          email: this.email,\n          password: this.password\n        });\n        // Supposons que la réponse contient le pseudo de l'utilisateur\n        this.setUser(response.data.pseudo);\n        this.$router.push('/menu');\n      } catch (error) {\n        console.error(\"Erreur de connexion :\", error.response ? error.response.data : error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapActions","data","email","password","methods","login","response","post","setUser","pseudo","$router","push","error","console","message"],"sources":["C:\\wamp64\\www\\Chess\\frontend\\src\\components\\LoginChess.vue"],"sourcesContent":["<template>\r\n  <div class=\"auth-container\">\r\n    <div class=\"auth-card\">\r\n      <h2>Connexion</h2>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">Email :</label>\r\n          <input type=\"email\" id=\"email\" v-model=\"email\" required />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">Mot de passe :</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" required />\r\n        </div>\r\n        <button type=\"submit\">Se connecter</button>\r\n      </form>\r\n      <p>\r\n        Pas encore de compte ? <router-link to=\"/signup\">S'inscrire</router-link>\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(['setUser']),\r\n    async login() {\r\n      try {\r\n        const response = await axios.post('http://localhost:8000/login/', {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n        // Supposons que la réponse contient le pseudo de l'utilisateur\r\n        this.setUser(response.data.pseudo);\r\n        this.$router.push('/menu');\r\n      } catch (error) {\r\n        console.error(\"Erreur de connexion :\", error.response ? error.response.data : error.message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background-image: url('@/img/fondSite.jpg');\r\n  background-size: cover;\r\n  background-position: center;\r\n}\r\n\r\n.auth-card {\r\n  background-color: rgba(0, 0, 0, 1); /* Cadre noir avec opacité */\r\n  border: 2px solid #00ff00; /* Bordure fluo verte */\r\n  box-shadow: 0 0 15px #00ff00, 0 0 30px #00ff00; /* Ombre lumineuse */\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  width: 300px;\r\n}\r\n\r\n.auth-card h2 {\r\n  text-align: center;\r\n  color: #e0e0e0;\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  margin-bottom: 5px;\r\n  color: #e0e0e0;\r\n}\r\n\r\ninput {\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  background-color: #1f1f1f; /* Fond des inputs pour les rendre visibles */\r\n  color: #e0e0e0; \r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  background-color: #00ff00;\r\n  color: #121212;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #00cc00;\r\n}\r\n\r\na {\r\n  color: #bb86fc;\r\n  text-decoration: none;\r\n}\r\n\r\na:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGJ,UAAU,CAAC,CAAC,SAAS,CAAC,CAAC;IAC1B,MAAMK,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,8BAA8B,EAAE;UAChEL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF;QACA,IAAI,CAACK,OAAO,CAACF,QAAQ,CAACL,IAAI,CAACQ,MAAM,CAAC;QAClC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;MAC5B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACN,QAAO,GAAIM,KAAK,CAACN,QAAQ,CAACL,IAAG,GAAIW,KAAK,CAACE,OAAO,CAAC;MAC9F;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}